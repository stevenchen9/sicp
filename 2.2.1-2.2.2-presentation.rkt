;; Lists are created by chaining cons cells

;;       +---+---+     +---+---+     +---+---+     +---+---+
;;  ---->| * | *-+---->| * | *-+---->| * | *-+---->| * | / |
;;       +-|-+---+     +-|-+---+     +-|-+---+     +-|-+---+
;;         |             |             |             |
;;         V             V             V             V
;;       +---+         +---+         +---+         +---+
;;       | 1 |         | 2 |         | 3 |         | 4 |
;;       +---+         +---+         +---+         +---+


(define l1 (cons 1
                 (cons 2
                       (cons 3
                             (cons 4 null)))))

(define l2 (list 1 2 3 4))

(define l3 '(1 2 3 4))



